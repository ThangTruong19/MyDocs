{
  "name": "ngk-azure-ad-authentication",
  "version": "1.0.3",
  "dependencies": {
    "@microsoft/microsoft-graph-client": "~1.3.0",
    "adal-angular": "~1.0.16",
    "core-js": "~2.5.1",
    "unfetch": "4.1.0"
  },
  "devDependencies": {
    "@types/adal-angular": "^1.0.1",
    "@types/jasmine": "^2.6.0",
    "@types/js-cookie": "^2.1.0",
    "@types/node": "12.0.2",
    "@types/node-fetch": "2.5.0",
    "@types/sinon": "^2.3.4",
    "fork-ts-checker-webpack-plugin": "5.0.7",
    "husky": "^0.14.3",
    "jasmine-core": "^2.8.0",
    "karma": "^5.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-jasmine": "^3.3.1",
    "karma-webpack": "^4.0.2",
    "lint-staged": "^7.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.12.1",
    "serve": "11.3.2",
    "sinon": "^9.0.2",
    "ts-loader": "8.0.1",
    "tslint": "^5.7.0",
    "tslint-eslint-rules": "^4.1.1",
    "typescript": "3.9.6",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0"
  },
  "license": "Proprietary",
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "clean": "run-p clean:*",
    "clean:dist": "rm -rf dist",
    "clean:zip": "run-p clean:zip:*",
    "clean:zip:dist": "rm -rf $npm_package_name.zip",
    "clean:zip:src": "rm -rf $npm_package_name",
    "build": "run-s clean build:umd",
    "build:umd": "webpack --config webpack.config.js --mode production",
    "watch:umd": "webpack --config webpack.config.js --mode development --watch",
    "dev": "run-p -l watch:umd serve:dev",
    "serve:dev": "serve public",
    "lint": "tslint -p tsconfig.json ./src/**/*.ts",
    "precommit": "lint-staged",
    "test": "npm run lint && npm run test:single-run",
    "test:single-run": "karma start --single-run",
    "test:watch": "karma start",
    "zip": "run-s clean build zip:mkdir zip:copy zip:exec clean:zip:src",
    "zip:mkdir": "mkdir $npm_package_name",
    "zip:copy": "cp -r src dist README.md package.json $npm_package_name",
    "zip:exec": "zip $npm_package_name.zip -r $npm_package_name"
  }
}
